   "source": [
    "def solution(lottos, win_nums):\n",
    "    rank = [6,6,5,4,3,2,1]\n",
    "    ans = 0\n",
    "    for i in lottos:\n",
    "        if(i in win_nums):\n",
    "            ans += 1\n",
    "    return [rank[(ans+lottos.count(0))], rank[ans]]"
   ]
  }
